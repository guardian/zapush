<configuration>

    <contextName>zapush (test)</contextName>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date [%thread] %-5level %logger{36} - %msg%n%xException{10}</pattern>
        </encoder>
      </appender>


    <root level="ALL">
        <appender-ref ref="CONSOLE"/>
    </root>

</configuration>